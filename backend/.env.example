# Configuración de Ambiente
NODE_ENV=development
PORT=3000

# Configuración de Base de Datos PostgreSQL
DB_NAME=tu_base_de_datos
DB_USER=tu_usuario
DB_PASS=tu_contraseña
DB_HOST=localhost

# Configuración CORS - Orígenes permitidos para requests del frontend
# Separar múltiples orígenes con comas
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:4173,http://localhost:8080
CORS_CREDENTIALS=true

# Configuración de Seguridad
SALT_ROUNDS=10